<% content_for :head do %><link rel="stylesheet" media="all" href="/stylesheets/highlight.css"/><script type="text/javascript" src="/javascripts/highlight.pack.js"></script><script type="text/javascript">  hljs.initHighlightingOnLoad();</script><% end %><article>	<h2 class="article-title"><a href="<%= post.url%>"><%= post.title %></a></h2>	<ul class="meta">	  <li class="post-time"><time datetime="<%= post.published_at.strftime("%Y-%m-%d")%>"><%= post.published_at.strftime("%B %d, %Y")%></time></li>	  <li class="post-category">Category: <%if post.category_id.blank?%>Uncategoried<%else%><a href="/categories/<%=post.category.name%>"><%=post.category.name%></a><%end%></li>	  <% unless AppSettings.disqus_shortname.blank? %>	  <li class="post-comments"><a href="<%= post.url %>#disqus_thread" data-disqus-identifier="<%=post.id%>">Comments</a></li>	  <% end %>	  <% if admin? %>          	  <li>		<a class="admin edit-post" href="/posts/<%=post.id%>/edit">Edit</a>		<a class="admin del-post" href="/posts/<%=post.id%>" data-method="delete" data-confirm="are you sure?">Delete</a>	  </li>  	  <% end %>                      	</ul>        	<div class="output-html">	  <%= post.body_html %>	</div>	<% unless post.tags.blank? %><div class="meta-tags"><%= tag_links(post) %></div><% end %> 	<hr/>	<% unless AppSettings.disqus_shortname.blank? %>	<div id="disqus_thread"></div>	<script type="text/javascript">		<% if development? %> var disqus_developer = 1; <% end %>		var disqus_shortname = '<%=AppSettings.disqus_shortname%>'; 		var disqus_identifier = '<%=post.id%>';		/* * * DON'T EDIT BELOW THIS LINE * * */		(function() {			var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;			dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';			(document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);		})();		(function () {			var s = document.createElement('script'); s.async = true;			s.type = 'text/javascript';			s.src = 'http://' + disqus_shortname + '.disqus.com/count.js';			(document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);		})();	</script>	<% end %></article>